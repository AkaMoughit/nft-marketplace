                            <li>
                                <a href="chat?p1Id=<%- sessionData.profile.profile_id%>&p2Id=<%- profile.profile_id%>">
                                    <div class="icon"><i class="icofont-speech-comments"></i></div>
                                    <div class="text">
                                        <p>Send Message</p>
                                    </div>
                                </a>
                            </li>
                            <% } %>
