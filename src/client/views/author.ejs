<!-- ==========Profile Section Start Here========== -->
<section class="profile-section padding-top padding-bottom">
    <div class="container">
        <div class="section-wrapper">
            <div class="member-profile">
                <div class="profile-item">
                    <div class="profile-cover">
                        <img src="<%- profile.banner_url%>" alt="banner-pic" id="bannerPic">
                        <div class="edit-photo custom-upload">
                            <div class="file-btn"><i class="icofont-camera"></i>
                                Edit Photo
                            </div>
                            <input type="file" accept="image/*" id="bannerPicInput" name="bannerPicInput">
                        </div>
                    </div>
                    <div class="profile-information">
                        <div class="profile-pic">
                            <img src="<%- profile.picture_url%>" alt="DP" id="profilePic">
                            <div class="custom-upload">
                                <div class="file-btn">
                                        <span class="d-none d-lg-inline-block"> <i class="icofont-camera"></i>
