                                                                                <a hidden id="popup-trigger" href="#popup" data-effect="mfp-zoom-in"></a>
                                                                                <button data-transaction="create" data-auth="<%- sessionData.isAuth %>"
                                                                                        class="create-nft-button">Create Item
                                                                                </button>
                                                                                <button data-transaction="list" data-auth="<%- sessionData.isAuth %>"
                                                                                        class="create-nft-button">Create Item and list it
                                                                                </button>
                                                                            </div>
                                                                            <div id="popup" class="blue-popup mfp-with-anim mfp-hide"></div>
                                                                        </section>
                                                                    </div>
                                                                <% } %>
                                                            </div>
                                                        </div>

